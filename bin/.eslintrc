{
  "env": {
    "node": true,
    // Can't enable this until we upgrade node.
    "es6": false
  },
  "rules": {
    "no-extra-parens": [2, "functions"],
    "no-invalid-this": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "strict": [2, "global"]
  }
}
